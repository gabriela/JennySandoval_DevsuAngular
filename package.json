{
  "name": "angular-interview",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "node --loader ts-node/esm node_modules/jest/bin/jest.js",
    "test:watch": "node --loader ts-node/esm node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --loader ts-node/esm node_modules/jest/bin/jest.js --coverage",
    "test:edit": "jest src/app/pages/products/edit/edit.component.spec.ts",
    "test:edit:coverage": "jest src/app/pages/products/edit/edit.component.spec.ts --coverage --coverageReporters=text",
    "test:list": "jest src/app/pages/products/list/list.component.spec.ts",
    "test:list:coverage": "jest src/app/pages/products/list/list.component.spec.ts --coverage --coverageReporters=text",
    "test:main": "jest src/app/pages/main/main.component.spec.ts",
    "test:main:coverage": "jest src/app/pages/main/main.component.spec.ts --coverage --coverageReporters=text",
    "test:table": "jest src/app/shared/components/data-table/data-table.component.spec.ts",
    "test:table:coverage": "jest src/app/shared/components/data-table/data-table.component.spec.ts --coverage --coverageReporters=text",
    "test:service:product": "jest src/app/services/product/product.service.spec.ts",
    "test:service:product:coverage": "jest src/app/services/product/product.service.spec.ts --coverage --coverageReporters=text",
    "test:app": "jest src/app/app.component.spec.ts",
    "test:app:coverage": "jest src/app/app.component.spec.ts --coverage --coverageReporters=text"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.7",
    "@angular/cli": "^19.2.7",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2"
  }
}
